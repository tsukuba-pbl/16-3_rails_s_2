<p id="notice"><%= notice %></p>

<h1>Listing Uxsers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Avatar</th>
      <th>Remove avatar</th>
      <th>Avatar cache</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uxsers.each do |uxser| %>
      <tr>
        <td><%= uxser.name %></td>
        <td><%= uxser.avatar %></td>
        <td><%= uxser.remove_avatar %></td>
        <td><%= uxser.avatar_cache %></td>
        <td><%= link_to 'Show', uxser %></td>
        <td><%= link_to 'Edit', edit_uxser_path(uxser) %></td>
        <td><%= link_to 'Destroy', uxser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uxser', new_uxser_path %>
