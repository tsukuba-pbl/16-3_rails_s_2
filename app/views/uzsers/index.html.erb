<p id="notice"><%= notice %></p>
<%= @msg %>
<h1>ユーザー管理画面<%= @msg %></h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Name2</th>
      <th>Name3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uzsers.each do |uzser| %>
      <tr>
        <td><%= uzser.name %></td>
        <td><%= uzser.name2 %></td>
        <td><%= uzser.name3 %></td>
        <td><%= link_to 'Show', uzser %></td>
        <td><%= link_to 'Edit', edit_uzser_path(uzser) %></td>
        <td><%= link_to 'Destroy', uzser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uzser', new_uzser_path %>
